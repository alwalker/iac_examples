{
    "image": "outline_vscode",
    "remoteUser": "root",
    "workspaceMount": "source=${localWorkspaceFolder},target=/workspace,z,type=bind",
    "workspaceFolder": "/workspace",
    "containerEnv": {
        "AWS_REGION": "us-east-1",
        "AWS_PAGER": ""
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "streetsidesoftware.code-spell-checker",
                "shardulm94.trailing-spaces",
                "hashicorp.terraform",
                "hashicorp.hcl",
                "ms-kubernetes-tools.vscode-kubernetes-tools"
            ]
        }
    }
}