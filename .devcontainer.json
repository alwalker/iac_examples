{
    "image": "outline_vscode",
    "runArgs": [
        "--userns=keep-id"
    ],
    "remoteUser": "root",
    "workspaceMount": "source=${localWorkspaceFolder},target=/workspace,z,type=bind",
    "workspaceFolder": "/workspace",
    // ],
    // "containerEnv": {
    //     "MY_CONTAINER_VAR": "some-value-here",
    //     "MY_CONTAINER_VAR2": "${localEnv:SOME_LOCAL_VAR}"
    // },
    "customizations": {
        "vscode": {
            "extensions": [
                "streetsidesoftware.code-spell-checker",
                "shardulm94.trailing-spaces",
                "hashicorp.terraform",
                "hashicorp.hcl"
            ]
        }
    }
}